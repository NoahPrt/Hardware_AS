{
  "files.eol": "\n",
  "files.associations": {
    "*.csv": "csv (semicolon)"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "meta.decorator.ts entity.name.function.ts",
        "settings": {
          "foreground": "#ffd000"
        }
      },
      {
        "scope": "meta.decorator.ts punctuation.decorator.ts",
        "settings": {
          "foreground": "#ffd000"
        }
      }
    ]
  },
  "material-icon-theme.activeIconPack": "nest",
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.useFlatConfig": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0b2261",
    "activityBar.background": "#0b2261",
    "activityBar.foreground": "#00a2ff",
    "activityBar.inactiveForeground": "#ffffff",
    "activityBarBadge.background": "#ae00ff",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#0b2261",
    "statusBar.background": "#0b2261",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0b2261",
    "statusBarItem.remoteBackground": "#0b2261",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#0b2261",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#0b2261",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#215732",
  "postman.telemetry.enabled": false,
  "postman.account.region": "EU",
  "asciidoc.antora.showEnableAntoraPrompt": false,
  "postman.settings.dotenv-detection-notification-visibility": false
}
